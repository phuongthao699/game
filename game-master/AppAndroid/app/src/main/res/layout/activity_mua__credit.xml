<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mua_Credit">

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="172dp"
        android:layout_height="147dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_game" />

    <ImageButton
        android:id="@+id/ic_goi100Credit"
        android:layout_width="151dp"
        android:layout_height="169dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:srcCompat="@drawable/icqua" />

    <ImageButton
        android:id="@+id/ic_goi200Credit"
        android:layout_width="151dp"
        android:layout_height="169dp"
        android:layout_marginStart="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ic_goi100Credit"
        app:layout_constraintTop_toTopOf="@+id/ic_goi100Credit"
        app:srcCompat="@drawable/icqua" />

    <ImageButton
        android:id="@+id/ic_goi500Credit"
        android:layout_width="151dp"
        android:layout_height="169dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="@+id/ic_goi100Credit"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:srcCompat="@drawable/icqua" />

    <ImageButton
        android:id="@+id/ic_goi1000Credit"
        android:layout_width="151dp"
        android:layout_height="169dp"
        android:layout_marginStart="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ic_goi500Credit"
        app:layout_constraintTop_toTopOf="@+id/ic_goi500Credit"
        app:srcCompat="@drawable/icqua" />

    <Button
        android:id="@+id/dong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="@string/trang_chu"
        android:onClick="trangchu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/dong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:onClick="trangchu"
        android:text="@string/trang_chu"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.902"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="G贸i 100 Credit"
        app:layout_constraintTop_toBottomOf="@+id/ic_goi100Credit"
        tools:layout_editor_absoluteX="54dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="G贸i 200 Credit"
        app:layout_constraintTop_toBottomOf="@+id/ic_goi200Credit"
        tools:layout_editor_absoluteX="260dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="G贸i 500 Credit"
        app:layout_constraintTop_toBottomOf="@+id/ic_goi500Credit"
        tools:layout_editor_absoluteX="54dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="G贸i 1000 Credit"
        app:layout_constraintTop_toBottomOf="@+id/ic_goi1000Credit"
        tools:layout_editor_absoluteX="250dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>